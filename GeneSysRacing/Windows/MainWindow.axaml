				<TextBlock Text="GeneSys" Classes="Bold" FontSize="80" Foreground="White"/>
				<TextBlock Text="RACING" Classes="Bold" FontSize="80"/>
			</StackPanel>

			<ContentPresenter Grid.Row="1" DataContext="{Binding RaceContainer}">
				<ContentPresenter.ContentTemplate>
					<DataTemplate>
						<v:RaceStepContainerView />
					</DataTemplate>
				</ContentPresenter.ContentTemplate>
			</ContentPresenter>
			
		</Grid>
	</DockPanel>

</Window>
