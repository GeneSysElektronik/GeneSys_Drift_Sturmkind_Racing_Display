		<SolidColorBrush x:Key="RadioButtonCheckGlyphFillPointerOver" Color="{DynamicResource LightBlue}" />
		<SolidColorBrush x:Key="RadioButtonCheckGlyphFillPressed" Color="{DynamicResource LightBlue}" />

