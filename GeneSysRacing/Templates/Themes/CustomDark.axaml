<Style xmlns="https://github.com/avaloniaui"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	   x:Class="Templates.Themes.CustomDark">

	<Style.Resources>

		<Color x:Key="FrameBackground">#0A2946</Color>
		<Color x:Key="Gray">LightGray</Color>

		<SolidColorBrush x:Key="ThemeDisabledBrush" Color="#40FFFFFF"/>

		<SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{DynamicResource DeepBlue}" />
		<SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource LightBlue}" />
