		<SolidColorBrush x:Key="ThemeItemBackgroundBrush" Color="{DynamicResource DeepBlue}" />
		<SolidColorBrush x:Key="ThemeItemPointeroverBackgroundBrush" Color="{DynamicResource DeepBlue}" />

		<LinearGradientBrush x:Key="ThemeNavigationGradientBrush">
			<GradientStop Offset="0" Color="Transparent" />
			<GradientStop Offset="0.3" Color="{DynamicResource LightBlue}" />
			<GradientStop Offset="0.7" Color="{DynamicResource LightBlue}" />
			<GradientStop Offset="1" Color="Transparent" />
		</LinearGradientBrush>

